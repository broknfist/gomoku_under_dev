<?xml version="1.0" encoding="utf-8"?>
<Window
	x:Class="gomoku.Window1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="gomoku"
	Height="600"
	Width="600">
	<Grid
		x:Name="MainGrid">
		<Grid.RowDefinitions>
			<RowDefinition
				Height="0.1*" />
			<RowDefinition
				Height="0.9*" />
		</Grid.RowDefinitions>
		<Grid
			x:Name="Menu">
			<Button
				x:Name="OtherBtn"
				Content="Button"
				Width="149"
				Grid.Column="0"
				Grid.Row="0"
				HorizontalAlignment="Left"
				VerticalAlignment="Stretch"
				Margin="83,11,0,4"
				FontWeight="Bold" />
		</Grid>
		<Grid
			x:Name="Board"
			Grid.Row="1" />
	</Grid>
</Window>